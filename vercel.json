{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/natureza", "destination": "/natureza.html" },
    { "source": "/retratos", "destination": "/retratos.html" },
    { "source": "/animais", "destination": "/animais.html" },
    { "source": "/objetos", "destination": "/objetos.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}